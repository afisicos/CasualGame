{
  "expo": {
    "name": "CasualGameMobile",
    "slug": "CasualGameMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.casualgame.foodforfun",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FF9966"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "VIBRATE"
      ]
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fc0f1a0c-787e-499e-9074-fe04ae4be6a2"
      }
    }
  }
}
